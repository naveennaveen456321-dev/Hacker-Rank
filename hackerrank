//1.Basic Arithmetic Operations
#include <stdio.h>
int main() {
 int a,b;
    scanf("%d%d",&a,&b);
    printf("Sum: %d",a+b);
    printf("\nDifference: %d",a-b);
    printf("\nProduct: %d",a*b);
    printf("\nQuotient: %d",a/b);
    printf("\nRemainder: %d",a%b);
    return 0;
}
//2.Smart Shopper
#include <stdio.h>
int main() 
{
    int cost1,qua1,cost2,qua2,cost3,qua3,bud;
    scanf("%d%d%d%d%d%d%d",&cost1,&qua1,&cost2,&qua2,&cost3,&qua3,&bud);
    int a,b,c;
    float d;
    a=cost1*qua1;
    b=cost2*qua2;
    c=cost3*qua3;
    d=a+b+c;
    int tax;
    tax=d*0.10;
    float tottax=tax+d;
    if(bud>=tottax)
    {
        printf("Total (Before Tax): %.2f\n",d);
        printf("Total (After Tax): %.2f\n",tottax);
        printf("Within Budget\n");     
    }
    else
    {
        printf("Total (Before Tax): %.2f\n",d);
        printf("Total (After Tax): %.2f\n",tottax);
        printf("Over Budget");
    }
    return 0;
}
//3.MINIMUM CUTS TO DIVIDE A CIRCLE 2
  int numberOfCuts(int n) {
    if(n == 1){
        return 0;
    }
    if(n%2==0)
    {
        n=n/2;
        return n;
    }
    else
    {
        return n;
    }

}
